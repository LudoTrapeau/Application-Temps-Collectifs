                                        android:layout_height="wrap_content"
                                        android:padding="10dp"
                                        android:text="Cliquez sur un assistant maternel pour avoir la liste des enfants "
                                        android:textColor="@android:color/background_dark" />

                                    <ListView
                                        android:id="@+id/lvEnfByAM"
                                        android:layout_width="match_parent"
                                        android:layout_height="326dp"
                                        android:layout_margin="20dp"
                                        android:choiceMode="none"
                                        android:focusable="false"
                                        android:focusableInTouchMode="false" />
                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginTop="20dp"
                                android:gravity="right"
                                android:orientation="horizontal">

                                <Button
                                    android:id="@+id/btnResAsmat"
                                    android:layout_width="150dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginRight="10dp"
                                    android:layout_weight="1"
                                    android:background="#10bed3"
                                    android:shadowRadius="10"
                                    android:text="PREINSCRIRE ASSMATS"
                                    android:textColor="#FFFFFF"
                                    android:textSize="14sp" />

                                <Button
                                    android:id="@+id/btnResEnf"
                                    android:layout_width="150dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginRight="10dp"
                                    android:layout_weight="1"
                                    android:background="#10bed3"
                                    android:text="PREINSCRIRE ENFANTS"
                                    android:textColor="#FFFFFF"
                                    android:textSize="14sp" />

                                <Button
                                    android:id="@+id/btnResEtValidAsmat"
                                    android:layout_width="150dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginRight="10dp"
                                    android:layout_weight="1"
                                    android:background="#ee5d16"
                                    android:text="PREINSCRIRE ET VALIDER PRESENCE ASSMATS"
                                    android:textColor="#FFFFFF"
                                    android:textSize="14sp" />

                                <Button
                                    android:id="@+id/btnResEtValidEnfByAM"
                                    android:layout_width="150dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:background="#ee5d16"
                                    android:text="PREINSCRIRE ET VALIDER PRESENCE ENFANTS"
                                    android:textColor="#FFFFFF"
                                    android:textSize="14sp" />
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLayoutParent"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginRight="40dp"
                            android:layout_marginTop="50dp"
                            android:background="@drawable/fenetretempscoll"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="50dp"
                                android:background="@drawable/encadrechampslogin"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/textView37"
                                    android:layout_width="400dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="50dp"
                                    android:text="Liste des parents et de leurs enfants"
                                    android:textColor="#068ca9" />

                                <ImageView
                                    android:id="@+id/imageView5"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="50dp"
                                    android:padding="5dp"
                                    app:srcCompat="@drawable/asmat" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="7dp">

                                <EditText
                                    android:id="@+id/inputSearchParents"
                                    android:layout_width="200dp"
                                    android:layout_height="wrap_content"
                                    android:ems="10"
                                    android:hint="Votre recherche"
                                    android:inputType="textPersonName" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="7dp">

                                <TextView
                                    android:id="@+id/tvNbParents"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Nb parents selectionnée(s) : "
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <ListView
                                    android:id="@+id/lvParents"
                                    android:layout_width="300dp"
                                    android:layout_height="400dp"
                                    android:layout_margin="10dp"
                                    android:choiceMode="none"
                                    android:focusable="false"
                                    android:focusableInTouchMode="false" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical">

                                    <TextView
                                        android:id="@+id/tvEnfByParent"
                                        android:layout_width="match_parent"
                                        android:layout_height="wrap_content"
                                        android:padding="10dp"
                                        android:text="Cliquez sur un parent pour avoir la liste des enfants "
                                        android:textColor="@android:color/background_dark" />

                                    <ListView
                                        android:id="@+id/lvEnfByParent"
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:layout_margin="20dp"
                                        android:choiceMode="none"
                                        android:focusable="false"
                                        android:focusableInTouchMode="false" />
                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginTop="20dp"
                                android:gravity="right"
                                android:orientation="horizontal">

                                <Button
                                    android:id="@+id/btnResParents"
                                    android:layout_width="150dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginRight="10dp"
                                    android:layout_weight="1"
                                    android:background="#10bed3"
                                    android:text="PREINSCRIRE PARENTS"
                                    android:textColor="#FFFFFF"
                                    android:textSize="14sp" />

                                <Button
                                    android:id="@+id/btnResEnfantsParents"
                                    android:layout_width="150dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginRight="10dp"
                                    android:layout_weight="1"
                                    android:background="#10bed3"
                                    android:text="PREINSCRIRE ENFANTS"
                                    android:textColor="#FFFFFF"
                                    android:textSize="14sp" />

                                <Button
                                    android:id="@+id/btnResValidParents"
                                    android:layout_width="150dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginRight="10dp"
                                    android:layout_weight="1"
                                    android:background="#ee5d16"
                                    android:text="PREINSCRIRE ET VALIDER PRESENCE PARENTS"
                                    android:textColor="#FFFFFF"
                                    android:textSize="14sp" />

                                <Button
                                    android:id="@+id/btnResValidEnfantsParents"
                                    android:layout_width="150dp"
                                    android:layout_height="match_parent"
                                    android:layout_weight="1"
                                    android:background="#ee5d16"
                                    android:text="PREINSCRIRE ET VALIDER PRESENCE ENFANTS"
                                    android:textColor="#FFFFFF"
                                    android:textSize="14sp" />
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLayoutEnfant"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginRight="40dp"
                            android:layout_marginTop="50dp"
                            android:background="@drawable/fenetretempscoll"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="50dp"
                                android:background="@drawable/encadrechampslogin"
                                android:gravity="center_vertical"
                                android:orientation="horizontal">

                                <TextView
                                    android:id="@+id/textView36"
                                    android:layout_width="400dp"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="50dp"
                                    android:text="Liste de tous les enfants"
                                    android:textColor="#068ca9" />

                                <ImageView
                                    android:id="@+id/imageView4"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="50dp"
                                    android:padding="5dp"
                                    app:srcCompat="@drawable/famille" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="7dp">

                                <EditText
                                    android:id="@+id/inputSearchEnfants"
                                    android:layout_width="200dp"
                                    android:layout_height="wrap_content"
                                    android:ems="10"
                                    android:hint="Votre recherche"
                                    android:inputType="textPersonName" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="7dp">

                                <TextView
                                    android:id="@+id/tvNbEnfants"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Nb enfants selectionnée(s) : "
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <ListView
                                    android:id="@+id/listView1"
                                    android:layout_width="match_parent"
                                    android:layout_height="400dp"
                                    android:layout_margin="10dp" />

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_marginTop="20dp"
                                android:orientation="horizontal">

                                <Button
                                    android:id="@+id/btnResEnfants"
                                    android:layout_width="150dp"
                                    android:layout_height="match_parent"
                                    android:background="#10bed3"
                                    android:text="PREINSCRIRE ENFANTS"
                                    android:textColor="#FFFFFF" />

                                <Button
                                    android:id="@+id/btnResValidEnfants"
                                    android:layout_width="165dp"
                                    android:layout_height="match_parent"
                                    android:layout_marginLeft="10dp"
                                    android:background="#ee5d16"
                                    android:text="PREINSCRIRE ET VALIDER ENFANTS"
                                    android:textColor="#FFFFFF" />

                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/linearLayoutPartenaire"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_alignParentTop="true"
                            android:layout_centerHorizontal="true"
                            android:layout_marginRight="40dp"
                            android:layout_marginTop="50dp"
                            android:background="@drawable/fenetretempscoll"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="50dp"
                                android:background="@drawable/encadrechampslogin"
                                android:gravity="center_vertical"

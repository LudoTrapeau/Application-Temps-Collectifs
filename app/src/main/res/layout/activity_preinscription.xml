                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="50dp"
                                    android:text="Liste de tous les partenaires"
                                    android:textColor="#068ca9" />

                                <ImageView
                                    android:id="@+id/imageView4"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="50dp"
                                    android:padding="5dp"
                                    app:srcCompat="@drawable/famille" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="7dp">

                                <EditText
                                    android:id="@+id/inputSearchPartenaires"
                                    android:layout_width="200dp"
                                    android:layout_height="wrap_content"
                                    android:ems="10"
                                    android:hint="Votre recherche"
                                    android:inputType="textPersonName" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical"
                                android:padding="7dp">

                                <TextView
                                    android:id="@+id/tvNbPartenaire"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:text="Nb partenaires selectionnée(s) : "
                                    android:textStyle="bold" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:orientation="horizontal">

                                <ListView
                                    android:id="@+id/lvPart"
                                    android:layout_width="match_parent"
                                    android:layout_height="400dp"
                                    android:layout_margin="10dp"
                                    android:choiceMode="none"
                                    android:focusable="true"
                                    android:focusableInTouchMode="true" />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:layout_marginTop="50dp"
